{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/users-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/users-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/users-service",
  "projects": {
    "users-service": {
      "type": "application",
      "root": "apps/users-service",
      "entryFile": "main",
      "sourceRoot": "apps/users-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-service/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "apps/posts",
      "entryFile": "main",
      "sourceRoot": "apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts/tsconfig.app.json"
      }
    },
    "data-access-users": {
      "type": "library",
      "root": "libs/data-access-users",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-users/tsconfig.lib.json"
      }
    },
    "data-access-posts": {
      "type": "library",
      "root": "libs/data-access-posts",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-posts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-posts/tsconfig.lib.json"
      }
    }
  }
}