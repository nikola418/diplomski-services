{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/users/src",
  "defaultLibraryPrefix": "@libs",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/users/tsconfig.app.json",
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "introspectComments": true,
          "classValidatorShim": true
        }
      }
    ]
  },
  "monorepo": true,
  "root": "apps/users",
  "projects": {
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "locations": {
      "type": "application",
      "root": "apps/locations",
      "entryFile": "main",
      "sourceRoot": "apps/locations/src",
      "compilerOptions": {
        "tsConfigPath": "apps/locations/tsconfig.app.json"
      }
    },
    "data-access-users": {
      "type": "library",
      "root": "libs/data-access-users",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-users/tsconfig.lib.json"
      }
    },
    "data-access-locations": {
      "type": "library",
      "root": "libs/data-access-locations",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-locations/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-locations/tsconfig.lib.json"
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    },
    "data-access-files": {
      "type": "library",
      "root": "libs/data-access-files",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-files/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-files/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "groups": {
      "type": "application",
      "root": "apps/groups",
      "entryFile": "main",
      "sourceRoot": "apps/groups/src",
      "compilerOptions": {
        "tsConfigPath": "apps/groups/tsconfig.app.json"
      }
    },
    "chats": {
      "type": "application",
      "root": "apps/chats",
      "entryFile": "main",
      "sourceRoot": "apps/chats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chats/tsconfig.app.json"
      }
    },
    "data-access-chats": {
      "type": "library",
      "root": "libs/data-access-chats",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-chats/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-chats/tsconfig.lib.json"
      }
    },
    "trips": {
      "type": "application",
      "root": "apps/trips",
      "entryFile": "main",
      "sourceRoot": "apps/trips/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trips/tsconfig.app.json"
      }
    },
    "data-access-trips": {
      "type": "library",
      "root": "libs/data-access-trips",
      "entryFile": "index",
      "sourceRoot": "libs/data-access-trips/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access-trips/tsconfig.lib.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    }
  }
}